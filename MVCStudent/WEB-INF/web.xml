<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://jakarta.ee/xml/ns/jakartaee"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://jakarta.ee/xml/ns/jakartaee http://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd"
version="5.0">


<display-name>Student Registration App</display-name>


<servlet>
<servlet-name>StudentController</servlet-name>
<servlet-class>com.example.controller.StudentController</servlet-class>
</servlet>


<servlet-mapping>
<servlet-name>StudentController</servlet-name>
<url-pattern>/register</url-pattern>
</servlet-mapping>


<welcome-file-list>
<welcome-file>register.jsp</welcome-file>
</welcome-file-list>


</web-app>